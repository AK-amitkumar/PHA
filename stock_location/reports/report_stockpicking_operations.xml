<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="spr_report_picking" inherit_id="stock.report_picking">
            <xpath expr="//table[2]/thead/tr/th[1]" position="after">
                <th width="15%">Localisation</th>
            </xpath>
            <xpath expr="//table[2]/tbody/tr/td[1]" position="after">
                   <td>

                    <strong>travée :</strong>
                    <span t-field="move.product_id.travee"/>
                    <br/>
                    <strong>colonne :</strong>
                    <span t-field="move.product_id.colonne"/>
                    <br/>
                    <strong>etagére :</strong>
                    <span t-field="move.product_id.etagere"/>
                    <br/>
                </td>
            </xpath>
        </template>
    </data>
</odoo>